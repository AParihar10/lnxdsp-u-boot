/dts-v1/;

#include "sc594-som.dtsi"

/ {
	model = "ADI sc594-som-ezkit";
	compatible = "adi,sc594-som-ezkit", "adi,sc59x";
};

&i2c2{
	gpio_expander1: mcp23017@21 {
		compatible = "microchip,mcp23017";
		reg = <0x21>;
		gpio-controller;
		#gpio-cells = <2>;
		u-boot,dm-pre-reloc;

		led1 {
			gpio-hog;
			gpios = <0 GPIO_ACTIVE_HIGH>;
			output-high;
			line-name = "led1-en";
			u-boot,dm-pre-reloc;
		};

		led2 {
			gpio-hog;
			gpios = <1 GPIO_ACTIVE_HIGH>;
			output-high;
			line-name = "led2-en";
			u-boot,dm-pre-reloc;
		};

		led3 {
			gpio-hog;
			gpios = <2 GPIO_ACTIVE_HIGH>;
			output-high;
			line-name = "led3-en";
			u-boot,dm-pre-reloc;
		};

		spi2flash-cs {
			gpio-hog;
			gpios = <3 GPIO_ACTIVE_HIGH>;
			output-low;
			line-name = "~spi2flash-cs";
			u-boot,dm-pre-reloc;
		};

		spi2d2-d3 {
			gpio-hog;
			gpios = <4 GPIO_ACTIVE_HIGH>;
			output-low;
			line-name = "~spi2d2-d3-en";
			u-boot,dm-pre-reloc;
		};

		uart0 {
			gpio-hog;
			gpios = <5 GPIO_ACTIVE_HIGH>;
			output-low;
			line-name = "~uart0-en";
			u-boot,dm-pre-reloc;
		};

		uart0-flow-en {
			gpio-hog;
			gpios = <6 GPIO_ACTIVE_HIGH>;
			output-high;
			line-name = "~uart0-flow-en";
			u-boot,dm-pre-reloc;
		};

		ospiflash-cs {
			gpio-hog;
			gpios = <7 GPIO_ACTIVE_HIGH>;
			output-low;
			line-name = "~ospiflash-cs";
			u-boot,dm-pre-reloc;
		};
	};
};
